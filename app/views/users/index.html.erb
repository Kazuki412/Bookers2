<% #ログイン中のユーザーの情報 %>
<%= render 'list2', user: @user %>

<% #新規投稿の共通化 %>
<%= render 'books/list', book: @book %>

<% #登録しているユーザー一覧 %>
<h1>Users</h1>
<% @users.each do |user| %>
  <h4>image</h4>
  <%= image_tag user.get_profile_image(100,100) %>
  <h4>name</h4>
  <%= user.name %>
  <%= link_to 'show', user_path(user.id) %>
<% end %>
