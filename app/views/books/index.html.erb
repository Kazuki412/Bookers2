<h1>Books</h1>
<% @books.each do |book| %>
  <h4>Title</h4>
  <p><%= image_tag book.user.get_profile_image(100,100) %></p>
  <span><%= link_to book.title, book_path(book.id) %></span>

  <h4>Opinion</h4>
  <span><%= book.body %></span>
<% end %>

